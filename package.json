{
  "name": "docs.floofy.dev",
  "description": "✏️ Documentation site for my projects",
  "private": true,
  "version": "2.0.0",
  "author": "August <cutie@floofy.dev>",
  "homepage": "https://docs.floofy.dev",
  "bugs": "https://github.com/auguwu/docs/issues",
  "scripts": {
    "clean:node_modules": "rm -rf node_modules/**/node_modules && rm -rf node_modules/@types/**/node_modules && rm -rf node_modules/@augu/**/node_modules",
    "clean:win:tar": "cp node_modules/@augu/collections/build/index.js.* node_modules/@augu/collections/build/index.js && rm node_modules/@augu/collections/build/index.js.*",
    "docker:build": "docker build . -t auguwu/docs:latest",
    "prepare": "npm run clean:node_modules",
    "build": "npm run lint && rm -rf build && tsc",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "dev": "cd src && ts-node --files --project ../tsconfig.json main.ts"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@augu/collections": "1.0.4",
    "@augu/lilith": "1.2.0",
    "@augu/utils": "1.4.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "simple-git": "2.37.0",
    "source-map-support": "0.5.19",
    "tslog": "3.1.2",
    "typedoc": "0.20.34"
  },
  "devDependencies": {
    "@augu/eslint-config": "1.10.0",
    "@augu/tsconfig": "1.0.1",
    "@types/express": "4.17.11",
    "@types/node": "14.14.37",
    "@types/rimraf": "3.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.23.0",
    "typescript": "4.2.3"
  }
}
